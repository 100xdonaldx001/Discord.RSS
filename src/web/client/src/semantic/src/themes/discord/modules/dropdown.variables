@blurple: #7289DA;
@greyple: #99AAB5;
@darkButNotBlack: #2C2F33;
@notQuiteblack: #23272A;

@selectionBackground: #303339;
@selectionVisibleTextColor: #aaabad;
@selectionTextColor: white;
@menuBackground: #18191c;
@menuBorder: 1px solid #040405 !important;
@selectedColor: #f6f6f7;

@errorBackgroundColor: #303339;
@errorBorderColor: rgba(240,71,71,.3);
@errorTextColor: #f04747;

/* selected item color */
@activeItemColor: white;
@selectedBackground: #131416;


/* non-selected item */
