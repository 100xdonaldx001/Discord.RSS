@blurple: #7289DA;
@greyple: #99AAB5;
@darkButNotBlack: #2C2F33;
@notQuiteblack: #23272A;

@red: #f04747;
@green: #43b581;
@redHover: darken(@red, 2.5%);
@redDown: darken(@red, 5%);
@redFocus: @red;
@background: @blurple;
@textColor: white;
@hoverBackgroundColor: darken(@background, 2.5%);
@hoverColor: white;
@focusBackgroundColor: @blurple;
@focusColor: white;
@downBackgroundColor: darken(@background, 5%);
@downColor: white;